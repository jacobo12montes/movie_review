<% @favmovie.each do |fav| %>
    <% if fav.user_id == current_user.id %>
        <%= fav.movie.title %>
    <% end %>    
    <% end %>
    
    
    
    <table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Movie lenght</th>
      <th>Director</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @favmovie.each do |fav| %>
      <tr>
    <% if fav.user_id == current_user.id %>
        <td><%= fav.movie.title %></td>
        <td><%= fav.movie.description %></td>
        <td><%= fav.movie.movie_lenght %></td>
        <td><%= fav.movie.director %></td>
        <td><%= fav.movie.rating %></td>

    <% end %>    
      </tr>
    <% end %>
  </tbody>
</table>